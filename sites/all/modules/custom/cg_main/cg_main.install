  <?php

  /**
   * @file
   * Hooks and enable code for Continuous Good.
   */

  /**
   * Implements hook_enable($modules).
   */
  function hook_custom_theme(cg_theme) {
  // Allow the user to request a particular theme via a query parameter.
  if (isset($_GET['cg_theme'])) {
    return $_GET['cg_theme'];
  }
}
